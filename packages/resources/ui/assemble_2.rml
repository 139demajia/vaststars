                color: aquamarine;
            }
            .build-picture {
                margin: 0.1em;
                width: 10.67vmin;
                height: 10.67vmin;
                background-color: rgb(191,144,0);
                background-size: cover;
                background-image: "construct/assembler.png";
                border: 0.27vmin rgb(92, 184, 92);
            }
            .progress-pic {
                width: 4.00vmin;
                height: 4.00vmin;
                background-size:cover;
                background-image:"character/flash.png";

            }
            .progress-bar-block {
                margin:0 0 0.05em 0;
                width: 80%;
                height:50%;
                justify-content: flex-start;
                border: 0.13vmin #fff;
            }
            .progress-background {
                justify-content: flex-start;
                width: 60%;
                height: 100%;
                background-color: rgb(87, 238, 0);
            }
            .percent-num {
                text-align:center; 
                position:absolute; 
                z-index: 3; 
                font-size: 4.00vmin; 
                top: -6px; 
                color: rgb(255, 255, 255);
                -webkit-text-stroke: 0.20vmin rgb(39, 32, 17); 
            }
            .change-recipe {
                margin:0 0 0 3em;
                align-items: center;
                justify-content: space-between;
                width: 10.67vmin;
                height: 10.67vmin;
                background-image:"science/book.png";
                background-size:cover;
            }
            .item-page-block {
                transition: left 0.5s linear-out; 
                width:100%;
                height:100%;
            }
            .build-info-reigon {
                width: 100%;
                height:75%;
                flex-direction: row;
                align-items: center;
                justify-content: space-evenly;
            }
            .build-model-block {
                margin-top: 2em;
                width: 44%;
                height:66%;
                flex-direction: column;
                align-items: center;
                justify-content: center;
            }
            .build-model {
                width:90%;
                height:90%;
                background-size: cover;
                background-image: "assemble/engine.png";
            }
            .power-block {
                margin-top: 0.3em;
                flex-direction: row;
                justify-content: center;
                align-items: flex-start;
                width:100%;
                height:10%;
            }
            .info-block {
                margin-top:1em;
                width: 28%;
                height:80%;
                flex-direction: column;
                align-items: center;
                justify-content: space-evenly;
            }
            .small-item {
                margin:0.1em;
                flex-direction: column;
                align-items:flex-start;
                justify-content:flex-start;
                background-color: rgb(250, 205, 9);
                border: 0.27vmin rgb(89, 73, 39);
                background-size:cover;
                width: 9.00vmin;
                height: 9.00vmin;
                font-size: 2.00vmin;
                background-image: "construct/insert1.png";
            }
            .exchange-region {
                flex-direction: column;
                align-items: center;
                justify-content: center;
                width:100%;
                height:20%;
                background-color: rgb(40, 40, 40);;
                border: 0.22vmin rgb(89, 73, 39);
            }
            .exchange-title {
                flex-direction: row;
                font-size: 42px;
                text-align: center;
                align-items: center;
                justify-content: center;
                height:35%;
            }
            .button-exchange-block {
                flex-direction: row;
                width: 100%;
                height:65%;
                align-items: center;
                justify-content: space-evenly;
                background-color: rgb(85, 85, 85);
            }
            .button-exchange {
                flex-direction: row;
                justify-content: center;
                align-items: center;
                width: 22.00vmin;
                height: 9.53vmin;
                background-color: rgb(52, 207, 115);
            }
            .button-exchange-text {
                font-size: 3.73vmin;
                text-align: center;
                color: rgb(255,249,214);
            }
            .button-exchange-arrow {
                width: 7.31vmin;
                height: 5.64vmin;
                background-size: cover;
                background-image: "exchange/left_arrow.png";
            }
            .button-exchange-box {
                width: 7.00vmin;
                height: 7.00vmin;
                background-size: cover;
                background-image: "exchange/yellow_box.png";
            }
        </style>
        <script type="text/x-lua" >
            local ui_sys = require "ui_system"
            console.log("before createModel")
            local start = window.createModel "start" {
                show = false,
                scale_flag = false,
                event_state = 'none',
                info_flag = true,
                fuel_interaction = false,
                material_interaction = false,
                plugin_interaction = false,
                reset = false,

                items = {
                    {id=100, icon0="correct.png", icon1="correct.png"},
                    {id=101, icon0="correct.png", icon1="correct.png"},
                    {id=102, icon0="correct.png", icon1="correct.png"},
                    {id=103, icon0="correct.png", icon1="correct.png"},
                    {id=104, icon0="correct.png", icon1="correct.png"},
                    {id=105, icon0="correct.png", icon1="correct.png"},
                    {id=106, icon0="correct.png", icon1="correct.png"},
                    {id=107, icon0="correct.png", icon1="correct.png"},
                    {id=108, icon0="correct.png", icon1="correct.png"},
                    {id=109, icon0="correct.png", icon1="correct.png"},
                    {id=110, icon0="correct.png", icon1="correct.png"},
                    {id=111, icon0="correct.png", icon1="correct.png"},
                    {id=112, icon0="correct.png", icon1="correct.png"},
                    {id=113, icon0="correct.png", icon1="correct.png"},
                    {id=114, icon0="correct.png", icon1="correct.png"},
                    {id=115, icon0="correct.png", icon1="correct.png"},
                    {id=116, icon0="correct.png", icon1="correct.png"},
                    {id=117, icon0="correct.png", icon1="correct.png"},
                    {id=118, icon0="correct.png", icon1="correct.png"},
                    {id=119, icon0="correct.png", icon1="correct.png"},
                    {id=120, icon0="correct.png", icon1="correct.png"},
                    {id=121, icon0="correct.png", icon1="correct.png"},
                    {id=122, icon0="correct.png", icon1="correct.png"},
                    {id=123, icon0="correct.png", icon1="correct.png"},
                    {id=124, icon0="correct.png", icon1="correct.png"},
                    {id=125, icon0="correct.png", icon1="correct.png"},
                    {id=126, icon0="correct.png", icon1="correct.png"},
                    {id=127, icon0="correct.png", icon1="correct.png"},
                    {id=128, icon0="correct.png", icon1="correct.png"},
                    {id=129, icon0="correct.png", icon1="correct.png"},
                },
                item_size = 90,
                view_items = {},
                virtual_pages = {},
                current_page = 0,
                selected_id = 100,
            }
            function start.mousedown(event)
                start.scale_flag = not start.scale_flag
                local style = event.current.style
                
                --[[
                if start.scale_flag then
                    style.top = '0%'
                    style.left = '0%'
                    style.width = '30%'
                    style.height = '30%'
                else
                    style.top = '0%'
                    style.left = '0%'
                    style.width = '20%'
                    style.height = '20%'
                end
                ]]

                start.show = start.scale_flag and true or false
                start.event_state = start.scale_flag and 'auto' or 'none'
            end

            function start.mousedown2(event)
                console.log("---mousedown2---")
            end

            function start.ClickFuel(event)
                start.info_flag = not start.info_flag
                start.fuel_interaction = not start.fuel_interaction
                console.log("---ClickFuel---")
                ui_sys.open("pipe_build_pop.rml")
            end

            function start.ClickMaterial(event)
                start.info_flag = not start.info_flag
                start.material_interaction = not start.material_interaction
                console.log("---ClickMaterial---")
                ui_sys.open("exchange_pop.rml")
            end

            function start.ClickPlugin(event)
                start.info_flag = not start.info_flag
                start.plugin_interaction = not start.plugin_interaction
                console.log("---ClickPlugin---")
            end
            
            function start.ClickBack(event)
                start.info_flag = true
                start.fuel_interaction = false
                start.material_interaction = false
                start.plugin_interaction = false
            end
            --------------------------
            local pageclass = require "page"
            -- window size : 1336 750
            local page_proxy = pageclass.create({540, 490}, start)
            function start.pagemousedown(event)
                console.log("pagemousedown")
                page_proxy:on_mousedown(event)
            end
            
            function start.pagedrag(event)
                --console.log("pagedrag")
                page_proxy:on_drag(event)
            end

            function start.pagemouseup(event)
                console.log("pagemouseup")
                page_proxy:on_mouseup(event)
                start.current_page = page_proxy:get_current_page() - 1
            end

            function start.page_item_click(event, id)
                console.log("page_item_click", id)
                start.selected_id = id
            end

            window.addEventListener('message', function(event)
                console.log("event.source.postMessage")
                event.source.postMessage(event.data)
            end)
            window.addEventListener('mousemove', function(event)
            end)
        </script>
    </head>
