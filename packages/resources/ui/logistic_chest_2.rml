<rml>
    <head>
        <style>
            body {
                font: 100% "宋体";
            }

            .items-region {
                width: 100%;
                height: 100%;
                border: 1px rgb(89, 73, 39);
                flex-direction: column;
                justify-content: center;
                align-items: center;
            }
            .single-item-block {
                width: 90%;
                height: 17%;
                border: 1px rgb(89, 73, 39);
                border-radius: 0.57vmin;
                flex-direction: row;
                justify-content: flex-start;
                align-items: center;
                margin:0.3vmin 0;
                background-color:burlywood;
            }
            .region-title {
                width: 33%;
                height: 100%;
                -webkit-text-stroke: 0.2vmin #000;
                flex-direction: row;
                justify-content: center;
                align-items: center;
                font-size: 5vmin;
                text-align: center;
            }
            .single-item {
                flex-direction: column;
                align-items:center;
                justify-content:center;
                background-size:cover;
                height:100%;
                width:14vmin;
            }
            .single-item-icon {
                width: 8.00vmin;
                height: 8.00vmin;
                background-image: "textures/construct/chest.texture";
                background-size: cover;
            }
            .single-item-title {
                font-size: 2.80vmin;
                text-align: center;
                color:rgb(255, 255, 255);
                -webkit-text-stroke: 0.3vmin rgb(0, 0, 0);
                width: 11.33vmin;
                margin: 0.00vmin;
            }
        </style>
        <style path = "common/building_style.rcss"/>
        <script type="text/x-lua" >
            local ui_sys = require "ui_system"
            local start = ui_sys.createDataMode("start", {
            })

            function start.ClickClose()
                ui_sys.close()
            end

        </script>
    </head>
    <body>
        <div class="item-container-region"  data-model="start">
            <!-- 左边面板 begin -->
            <div class="panel" style="width: 45%;">
                <div class="header" id = "item-name-block">
                    <button class = "back-button" data-event-click="ClickClose()"/>
                    <div class = "menu-title" style="width: 70%;">货运仓库</div>
                </div>
                <div style="border-bottom: 1px rgb(89, 73, 39);"/>
                <div class="column" style="width: 100%;height: 93%;">
                    <div class="build-info-reigon" style='background-image: "textures/build_background/pic_chest.texture"; background-size: cover;'>
                        <div class="build-model-block">
                            <div class="build-model"/>
                        </div>
                    </div>
                </div>
            </div>
            <!-- 左边面板 end -->

            <!-- 右边面板 begin -->
            <div class="panel" style="width: 45%;">
                <div class="header" style="justify-content: flex-end; border: 0.13vmin rgb(89, 73, 39);">
                    <div class="menu-title" style="flex-direction: row; align-items: center; width:auto;">容纳货物</div>
                </div>
                <div class="column" style="width: 100%;height: 93%;">
                    <!-- 物品栏显示 start -->
                    <div class="item-container" style="height: 80%;">
                        <div style="width:100%; overflow: hidden;">
                            <div class = "items-region" style="justify-content:flex-start;">
                                <div class="single-item-block" style="height:10%; background-color: transparent; border: 0vmin; justify-content: flex-end;">
                                    <div class="region-title" style="font-size:4vmin;">库存： 100/100</div>
                                </div>
                                <div class="single-item-block">
                                    <div class="single-item">
                                        <div class="single-item-icon"/>
                                        <div class="single-item-title">储藏箱</div>
                                    </div>
                                    <div class="progress-bar-block" style = "margin:0 0.2vmin; width: 70%; height:50%;">
                                        <div class="progress-background"/>
                                        <div class = "percent-num" style="position:absolute; left:18.5vmin; top:0.5vmin;">100/100</div>
                                    </div>   
                                </div>
                                <div class="single-item-block">
                                    <div class="single-item">
                                        <div class="single-item-icon"/>
                                        <div class="single-item-title">储藏箱</div>
                                    </div>
                                    <div class="progress-bar-block" style = "margin:0 0.2vmin; width: 70%; height:50%;">
                                        <div class="progress-background"/>
                                        <div class = "percent-num" style="position:absolute; left:18.5vmin; top:0.5vmin;">100/100</div>
                                    </div>  
                                </div>
                                <div class="single-item-block">
                                    <div class="single-item">
                                        <div class="single-item-icon"/>
                                        <div class="single-item-title">储藏箱</div>
                                    </div>
                                    <div class="progress-bar-block" style = "margin:0 0.2vmin; width: 70%; height:50%;">
                                        <div class="progress-background"/>
                                        <div class = "percent-num" style="position:absolute; left:18.5vmin; top:0.5vmin;">100/100</div>
                                    </div>  
                                </div>
                                <div class="single-item-block">
                                    <div class="single-item">
                                        <div class="single-item-icon"/>
                                        <div class="single-item-title">储藏箱</div>
                                    </div>
                                    <div class="progress-bar-block" style = "margin:0 0.2vmin; width: 70%; height:50%;">
                                        <div class="progress-background"/>
                                        <div class = "percent-num" style="position:absolute; left:18.5vmin; top:0.5vmin;">100/100</div>
                                    </div>  
                                </div>
                                <div class="single-item-block">
                                    <div class="single-item">
                                        <div class="single-item-icon"/>
                                        <div class="single-item-title">储藏箱</div>
                                    </div>
                                    <div class="progress-bar-block" style = "margin:0 0.2vmin; width: 70%; height:50%;">
                                        <div class="progress-background"/>
                                        <div class = "percent-num" style="position:absolute; left:18.5vmin; top:0.5vmin;">100/100</div>
                                    </div>  
                                </div>
                            </div>
                        </div>      
                    </div>
                    <!-- 物品栏显示 end -->

                    <!-- 物流需求栏显示 start -->

                    <div class="row" data-event-click="false" style="border: 0.13vmin rgb(89, 73, 39); width:100%; height: 15%; justify-content: center; align-items: center;">                       
                        <button class="single-item-block" style="height: 80%; justify-content:center; background-color: rgb(246, 142, 14);">
                            <div class="item-add" style="background-color: transparent; border: 0px; width:10vmin; height:10vmin;"/>
                        </button>
                    </div>

                    <div class="row" data-event-click="false" style="border: 0.13vmin rgb(89, 73, 39); width:100%; height: 15%; justify-content: center; align-items: center;" data-if="false">
                        <button class="item">
                            <div class="single-item-icon" style="width:12vmin; height:12vmin;"/>
                            <div class ="item-count">7</div>
                        </button>                 
                        <button class="item">
                            <div class="single-item-icon" style="width:12vmin; height:12vmin;"/>
                            <div class ="item-count">7</div>
                        </button>                       
                        <button class="item">
                            <div class="single-item-icon" style="width:12vmin; height:12vmin;"/>
                            <div class ="item-count">7</div>
                        </button> 
                        <button class="item">
                            <div class="single-item-icon" style="width:12vmin; height:12vmin;"/>
                            <div class ="item-count">7</div>
                        </button>                       
                        <button class="item">
                            <div class="single-item-icon" style="width:12vmin; height:12vmin;"/>
                            <div class ="item-count">7</div>
                        </button>                        
                        <button class="item-add"/>
                    </div>
                    <!-- 物流需求栏显示 end -->
                </div>
            </div>       
        </div>
    </body>
</rml>