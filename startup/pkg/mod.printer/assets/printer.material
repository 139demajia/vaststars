fx:
  vs: /pkg/ant.resources/shaders/pbr/vs_pbr.sc
  fs: /pkg/mod.printer/assets/shaders/fs_printer.sc # shoule be replaced by printer's pixel shader
  setting:
    ALPHAMODE_OPAQUE: 1
    lighting: on
    shadow_cast: on
    shadow_receive: on
    subsurface: off
properties:
  s_basecolor:
    stage: 0
    texture: /pkg/ant.resources/textures/default.texture # should be replaced by glb's own texture
  u_basecolor_factor: {1, 1, 1, 1}
  u_emissive_factor: {0, 0, 0, 0}
  u_pbr_factor: {0, 1, 0.5, 1}
  u_construct_color : {0.5, 0.5, 0.5, 1} # constructing part's color
  u_printer_factor:  {0.1, 0, 0, 0} # contructing gap/constructing topmost
state:
  ALPHA_REF: 0
  CULL: CCW
  DEPTH_TEST: GREATER
  MSAA: true
  WRITE_MASK: RGBAZ