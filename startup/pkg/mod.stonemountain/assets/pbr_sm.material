fx:
  shader_type: "PBR"
  setting:
    ALPHAMODE_OPAQUE: 1
    DRAW_INDIRECT: 1
    lighting: on
    shadow_cast: on
    shadow_receive: on
    subsurface: off
    WITH_COLOR_ATTRIB: 1
    WITH_TANGENT_ATTRIB: 1
    PACK_TANGENT_TO_QUAT: 1
properties:
  u_basecolor_factor:   {1, 1, 1, 1}
  u_emissive_factor:    {0, 0, 0, 0}
  u_pbr_factor:         {0, 1, 0.5, 1}
  u_draw_indirect_type: {0, 0, 0, 0}
state:
  ALPHA_REF: 0
  CULL: CCW
  DEPTH_TEST: EQUAL
  MSAA: true
  WRITE_MASK: RGBAZ