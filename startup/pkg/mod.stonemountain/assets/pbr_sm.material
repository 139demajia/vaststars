fx:
  shader_type: "PBR"
  setting:
    ALPHAMODE_OPAQUE: 1
    DRAW_INDIRECT: 1
    lighting: on
    shadow_cast: on
    shadow_receive: on
    subsurface: off
    WITH_COLOR_ATTRIB: 1
properties:
