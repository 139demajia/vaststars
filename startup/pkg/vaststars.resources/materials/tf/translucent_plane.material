fx:
  vs_code: "#include \"tf_vs_func.sh\""
  fs_code: "#include \"tf_fs_func.sh\""
  macros:
    "ALPHAMODE_OPAQUE=1"
  setting:
    lighting: off
  varyings:
    a_position: vec3
    a_texcoord0: vec2
    v_texcoord0: "vec2 TEXCOORD0"
    v_posWS: "vec4 TEXCOORD1"
properties:
  s_basecolor:
    stage: 0
    texture: /pkg/vaststars.resources/textures/tf/corner.texture 
state:
  ALPHA_REF: 1
  CULL: CCW
  DEPTH_TEST: ALWAYS
  MSAA: true
  WRITE_MASK: RGBA
  BLEND_FUNC: "aA"
  BLEND_ENABLE: "i"